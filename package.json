{
  "name": "bongocat-sidebar",
  "displayName": "BongoCat Sidebar",
  "description": "Coding with üê± !!",
  "icon": "media/bongo_icon.png",
  "publisher": "Milynn",
  "repository": {
    "type": "git",
    "url": "https://github.com/tobemini/bongocat-sidebar"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:bongocat-sidebar.bongoCat"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "bongocat-sidebar.bongoCat",
        "title": "Bongo Cat Activate"
      }
    ],
    "viewsContainers": {
      "sidebar": [
        {
          "id": "bongoCatContainer",
          "title": "Bongo Cat",
          "icon": "media/bongo_icon.png"
        }
      ]
    },
    "views": {
      "bongoCatContainer": [
        {
          "type": "webview",
          "id": "bongoCatView",
          "name": "Bongo Cat"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
